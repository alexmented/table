{
  "env": {
    "browser": true
  },
  "plugins": ["import", "react"],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "warn",
    "import/first": "error",
    "react/prop-types": 0
  },
  "parser": "babel-eslint"
}